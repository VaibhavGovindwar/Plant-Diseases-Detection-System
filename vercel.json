{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.10",
        "PIP_NO_CACHE_DIR": "off"
    },
    "postinstall": "pip install tensorflow-cpu==2.18.0"
}